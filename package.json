{
  "name": "computerization-website",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "src/plugins/*"
  ],
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,html,css}\"",
    "lint": "eslint --cache  --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,mjs}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "latest",
    "@docusaurus/preset-classic": "latest",
    "@docusaurus/theme-classic": "latest",
    "@mdx-js/react": "1.6.22",
    "clsx": "2.0.0",
    "docusaurus-plugin-alumni": "0.0.0",
    "prism-react-renderer": "1.3.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "rehype-katex": "5.0.0",
    "remark-math": "3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "latest",
    "@docusaurus/types": "latest",
    "@tsconfig/docusaurus": "2.0.0",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-jc": "2.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.0",
    "prettier": "3.0.3",
    "prettier-config-jc": "^2.3.0",
    "typescript": "4.7.3",
    "unified": "11.0.3"
  },
  "packageManager": "yarn@3.2.1"
}
